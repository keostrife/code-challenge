# custom file extensions
AddType application/x-httpd-php keo strife template view model helper partial request setting api service library

# route everything to index.php
Options +FollowSymLinks
IndexIgnore */*
RewriteEngine on
RewriteCond %{REQUEST_URI} !^((/index\.php)|(/index\.html)|(/robots\.txt)|(/public/)|(/humans\.txt)|(/sitemap\.xml))
RewriteRule ^(.*)$ index.html
